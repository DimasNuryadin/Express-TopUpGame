{
  "builds": [{
    "src": "/index.js",
    "use": "@vercel/node"
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "/index.js",
    "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTION"],
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Headers": "Origin, Content-Type, X-Auth-Token"
    }
  }]
}